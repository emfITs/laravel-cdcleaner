{
    "name": "emfits/cdcleaner",
    "description": "Cleaning up old releases that won't be needed anymore. ",
    "keywords": [
        "emfits",
        "cdcleaner"
    ],
    "homepage": "https://github.com/emfits/cdcleaner",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Marcel Nowocyn",
            "email": "m.nowocyn@emfits.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Emfits\\CDCleaner\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Emfits\\CDCleaner\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Emfits\\CDCleaner\\CDCleanerServiceProvider"
            ],
            "aliases": {
                "CDCleaner": "Emfits\\CDCleaner\\CDCleanerFacade"
            }
        }
    }
}
